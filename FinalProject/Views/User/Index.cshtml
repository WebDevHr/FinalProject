@model List<FinalProject.Service.Models.ApplicationUser>

<div class="d-flex flex-wrap">
@foreach(var item in Model) {
    <div class="card col-lg-3 col-md-6 col-12 m-md-2 my-2" >
        <div class="card-body">
            <h5 class="card-title">@item.FirstName @item.LastName</h5>
            <h6 class="card-subtitle mb-2 text-muted">@item.Email</h6>
            <h6 class="card-subtitle mb-2 text-muted">@item.PhoneNumber</h6>
            <h6 class="card-subtitle mb-2 text-muted">@item.PasswordHash</h6>
        </div>
    </div>
    }
</div>